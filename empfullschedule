var headerrow = null;
var headertop = 0;
var fixedHeaderRow = null;
var fixedHeaderDiv = null;
function getScrollTop(){
if(typeof pageYOffset!= 'undefined'){
//most browsers except IE before #9
return pageYOffset;
}
else{
var B= document.body; //IE 'quirks'
var D= document.documentElement; //IE with doctype
D= (D.clientHeight)? D: B;
return D.scrollTop;
}
}
function getAbsolutePos(el) {
var SL = 0, ST = 0;
var is_div = /^div$/i.test(el.tagName);
if (is_div && el.scrollLeft)
SL = el.scrollLeft;
if (is_div && el.scrollTop)
ST = el.scrollTop;
var r = { x: el.offsetLeft - SL, y: el.offsetTop - ST };
if (el.offsetParent) {
var tmp = getAbsolutePos(el.offsetParent);
r.x += tmp.x;
r.y += tmp.y;
}
return r;
};
function pageScrolled() { // called when the window is scrolled.
if (!headerrow.cells) return false;
var height = getScrollTop();
var tw = window.innerWidth || document.body.clientWidth;
var wpct;
if(height  > headertop) {
var daysTable = document.getElementById('daysTable');
var tbl = document.getElementById('headertable');
tbl.style.width = (daysTable.offsetWidth+1.2)+'px';
tbl.style.left = Math.max(((document.body.clientWidth-daysTable.offsetWidth)/2),1)+'px';
for (var j = 0, col; col = headerrow.cells[j]; j++) {
wpct = 100*(col.offsetWidth / tw).toFixed(4);
fixedHeaderRow.cells[j].style.width = (wpct)+'%';
}
fixedHeaderDiv.style.display = 'block';
} else {
fixedHeaderDiv.style.display = 'none';
}
}
function initFixedHeader() {
headerrow = document.getElementById('headerrow');
headertop = getAbsolutePos(headerrow).y;
fixedHeaderDiv = document.createElement("div");
fixedHeaderDiv.style.display = 'none';
fixedHeaderDiv.style.position = 'fixed';
var tbl=document.getElementById('daysTable').cloneNode(true);    //clone the table
var rowCount = tbl.rows.length;
for (var i = 1; i < rowCount; i++) {
tbl.deleteRow(1);
}
fixedHeaderRow = tbl.rows[0];
tbl.id = 'headertable';
tbl.style.tableLayout = 'fixed';
tbl.style.position = 'fixed';
tbl.style.left = '0px';
tbl.style.width = daysTable.offsetWidth+'px';
tbl.style.backgroundColor = 'white';
fixedHeaderDiv.appendChild(tbl);
var body = document.body||document.getElementsByTagName('body')[0];
body.insertBefore(fixedHeaderDiv,body.firstChild);
pageScrolled();
window.onresize=pageScrolled;
window.onscroll=pageScrolled;
}
</SCRIPT>
</head>
