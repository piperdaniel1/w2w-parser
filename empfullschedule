<script class='LayeredJS'>
function jmp(dt){
ReplWin('empfullschedule','&Date='+dt);
}
function jmpLayer(dt,parent){
parent=parent||false;
ReplWinLayer('empfullschedule','&Date='+dt,null,null,null,null,parent);
}
var jumpHandler = jump;
var navHandler = null;
var date_layer=true;
function openDatePage(isSND){
if((typeof date_layer !== 'undefined')&&(date_layer)) {
var onSelect = function(calendar, date){
if(calendar.dateClicked){
// get the value and do something
calendar.callCloseHandler();
var y = calendar.date.getFullYear();
var m = calendar.date.getMonth()+1;     // integer, 0..11 + 1
var d = calendar.date.getDate();      // integer, 1..31
// window.top.location=window.top.location+"&Date="+m + "/" + d + "/" + y;
jumpHandler(m + "/" + d + "/" + y);
}
};
var onClose = function(calendar){
calendar.hide(); // hide calendar
};
var navDatecal = new Calendar(1, null, onSelect, onClose); //create
var dateEl =document.getElementById("mgrDayCalSelect"+isSND);
navDatecal.weekNumbers = false;
navDatecal.setDateFormat("%m/%d/%Y");
navDatecal.create(null);//);
// navDatecal.parseDate(dateEl.value);
navDatecal.element.style.zIndex = 10000;
var pos = getRelativePositionEx(dateEl,navDatecal.element,'bottom');
navDatecal.showAt(pos.left,pos.top);
} else {
window.open('/cgi-bin/w2wE.dll/datepick.htm?SID=642607716419D', 'PopDate','scrollbars,menubars,statusbars,resizable,top=300,left=400,width=220,height=220');
}
}
var CurrentWeek = new Date('2022-10-03T00:00:00Z'); // Use "Z" to insure consistent behavior
CurrentWeek.setMinutes(CurrentWeek.getMinutes() + CurrentWeek.getTimezoneOffset()); // Then normalize
Date.prototype.addDays = function(days) {
var dat = new Date(this.valueOf());
dat.setDate(dat.getDate() + days);
return dat;
}
function updateNavDate() {
var el = document.getElementById('ndThisWeek');
el.innerHTML = 'Week of '+CurrentWeek.toLocaleDateString("en-US", {month: 'short'})+' '+CurrentWeek.getDate()+', '+CurrentWeek.getFullYear();
var dt;
for (var i=0;i<=6;i++) {
dt = CurrentWeek.addDays(i);
if (TheDates) TheDates[i] = (dt.getMonth()+1)+'/'+dt.getDate()+'/'+dt.getFullYear();
if (dn) dn[i] = dt.toLocaleDateString("en-US", {weekday: 'long'})+', '+dt.toLocaleDateString("en-US", {month: 'short'})+'-'+dt.getDate();
}
for (var i=1;i<=6;i++) {
el = document.getElementById('nd_-'+i);
dt = CurrentWeek.addDays(i*-7);
el.innerHTML = dt.toLocaleDateString("en-US", {month: 'short'})+'-'+dt.getDate();
}
for (var i=1;i<=6;i++) {
el = document.getElementById('nd_+'+i);
dt = CurrentWeek.addDays(i*7);
el.innerHTML = dt.toLocaleDateString("en-US", {month: 'short'})+'-'+dt.getDate();
}
}
function prevNavDate() {
CurrentWeek = navHandler(CurrentWeek.addDays(-7));
updateNavDate();
}
function nextNavDate() {
CurrentWeek = navHandler(CurrentWeek.addDays(7));
updateNavDate();
}
function gotoToday() {
CurrentWeek = navHandler(new Date());
updateNavDate();
}
function gotoNavDate(dt) {
var dtAr = dt.split('/');
CurrentWeek = navHandler(new Date(dtAr[2],parseInt(dtAr[0])-1,dtAr[1]));
updateNavDate();
}
function gotoNavDateOffset(el,offset) {
CurrentWeek = navHandler(CurrentWeek.addDays(offset*7));
updateNavDate();
}
function dynNavDate(fnNavHandler) { //NavDate.htm behaves exactly the same as before until/unless dynNavDate() is called
//Replace existing click-handlers with new
navHandler = fnNavHandler;
jumpHandler = gotoNavDate;
document.getElementById('ndToday').setAttribute('onclick','gotoToday()');
document.getElementById('ndPrevWeek').setAttribute('onclick','prevNavDate()');
document.getElementById('ndNextWeek').setAttribute('onclick','nextNavDate()');
for (var i=1;i<=6;i++) {
document.getElementById('nd_-'+i).setAttribute('onclick','gotoNavDateOffset(this,-'+i+')');
}
for (var i=1;i<=6;i++) {
document.getElementById('nd_+'+i).setAttribute('onclick','gotoNavDateOffset(this,'+i+')');
}
}
</script>
