function CellClicked(myElement,dir)
{
if (dir.charAt(0)=='#')
{ windowOpen('emptradeboardentry.htm','&Id='+dir.substring(1), 'tbentry') }
else if (dir.charAt(0)=='!')
{ windowOpen('empmytradeboardentry.htm','&Id='+dir.substring(1), 'tbmyentry') }
else if ((dir.charAt(0)=='*')||(dir.charAt(0)=='^'))
{ windowOpen('empopenshiftentry.htm','&TransactionId='+dir.substring(1), 'emposh') }
else if ((dir.charAt(0)=='$') || (dir.charAt(0)=='~'))
{ windowOpen('empscheduleentry.htm','&Id='+dir.substring(1), 'myshift') }
else
{ windowOpen('empallshiftentry.htm','&Id='+dir, 'eallshift') }
return false;
}
function ListChanged(el)
{
top.window.location.replace("/cgi-bin/w2wE.dll/empfullschedule?SID=642607716419D&SkillFilter="+el.value)
}
function CatListChanged(el)
{
top.window.location.replace("/cgi-bin/w2wE.dll/empfullschedule?SID=642607716419D&CatFilter="+el.value)
}
