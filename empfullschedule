function handleMenuKey(e,el) {
if (e.keyCode == 13) {
el.click();
}
}
function MenuListChanged(el)
{
if (el.value=='home') {ReplWin('home','')}
else if (el.value=='schedules') {ReplWin('empschedule','')}
else if (el.value=='fullweek') {ReplWin('empfullschedule','&View=Week')}
else if (el.value=='info') {ReplWin('empinfo.htm','')}
else if (el.value=='prefs') {ReplWin('emppreferences.htm','')}
else if (el.value=='timeoff') {ReplWin('emptimeoff.htm','')}
else if (el.value=='trades') {ReplWin('emptradeboard','')}
else if (el.value=='messaging') {ReplWin('empmessaging.htm','')}
else if (el.value=='staff') {ReplWin('empemplist.htm','')}
else if (el.value=='onnow') {ReplWin('empwhosonnow.htm','')}
else if (el.value=='bb') {ReplWin('empbb.htm','')}
else if (el.value=='onnow') {ReplWin('empwhosonnow.htm','')}
else if (el.value=='onnow') {ReplWin('empwhosonnow.htm','')}
else if (el.value=='help') {showW2WHelp()}
else if (el.value=='signout') {ReplWin('signoff','')}
}
function signoutClick() {
if (subMenuAction) {
//console.log('signout click ignored!');
subMenuAction = false;
return true;
}
//ReplWin('signoff','');
}
</script>
